<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
 
  <Target Name="Build">
    <MSBuild Projects="@(BuildProject)" StopOnFirstFailure="true"/>
  </Target>
  
  <Target Name="Clean">
    <MSBuild Projects="@(BuildProject)" StopOnFirstFailure="true"/>
  </Target>

  <Target Name="Rebuild">
    <MSBuild Projects="@(BuildProject)" StopOnFirstFailure="true"/>
  </Target>

</Project>

